{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy-ip.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 10 }
    },
    {
      "src": "api/send-ip.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 10 }
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/proxy-ip",
      "dest": "api/proxy-ip.js"
    },
    {
      "src": "/api/send-ip",
      "dest": "api/send-ip.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}