{
  "version": 2,
  "builds": [
    {
      "src": "api/send-notification.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/send-notification",
      "dest": "/api/send-notification.js",
      "methods": ["POST", "OPTIONS"]
    }
  ]
}